.stats-nav{
  width: 100%;
}
.stats-nav text{
  display: block;
  width: 20%;
  float: left;
  text-align: center;
  font-size: 28rpx;
  padding: 20rpx 0;
  color: #333
}

.table{
  width: 100%;
  float: left
}
.header{
  width: 100%;
  height: 70rpx;
  background: #f5f5f5;
  font-size:28rpx;
}

.header text{
  display: block;
  width: 33.33%;
  float: left;
  text-align: center;
  line-height: 70rpx;
}
.tbody{
  font-size: 28rpx;

}
.tbody text{
  display: block;
  width: 33.33%;
  float: left;
  text-align: center;
  line-height: 70rpx;
  height: 70rpx;
  overflow: hidden
}
.tbody text:first-child,.header text:first-child{
  text-align: left;
  box-sizing: border-box;
  padding-left: 20rpx;
  overflow: hidden
}
text.active{
  color: #00AA98
}
.tbody navigator{
  display: block;
  height: 70rpx;
}
.f5f5f5{
  background: #f5f5f5
}