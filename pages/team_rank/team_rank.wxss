.swiper-tab {
    width: 100%;
    border-bottom: 1rpx solid #e3e3e3;
    text-align: center;
    line-height: 80rpx;
}

.swiper-tab-list {
    font-size: 30rpx;
    display: inline-block;
    width: 50%;
    color: #777;
}

.on {
    color: #00aa98;
}

.swiper-box {
    display: block;
    width: 100%;
    box-sizing: border-box;
    height: auto;
    min-height: 100%;
    float: left;
}

.swiper-item {
    width: 100%;
    height: 100%;
    display: none;
    box-sizing: border-box;
}

view.active {
    display: block;
}

.t-head {
    background: #f2f7fc;
    font-size: 30rpx;
    height: 80rpx;
    line-height: 80rpx;
    color: #444
}

.t-head text,
.t-body .team-item text {
    width: 20%;
    display: block;
    float: left;
    text-align: center;
}

.t-body .team-item {
    font-size: 28rpx;
    height: 65rpx;
    line-height: 65rpx;
    border-top: 1rpx solid #e3e3e3;
    box-sizing: border-box;
    padding: 0 30rpx;
}

.t-body .team-item text:first-child,
.t-head text:first-child {
    text-align: left;
    font-size: 24rpx;
}